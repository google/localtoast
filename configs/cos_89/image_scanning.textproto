      "    files_to_check:{single_file:{path:\"/etc/cos-package-info.json\"}}"
      "    content_entry:{"
      "      match_type: NONE_MATCH"
      "      match_criteria: {"
      "        filter_regex: \".*\\\"name\\\": \\\"ypbind\\\".*\""
      "        expected_regex: \".*\\\"name\\\": \\\"ypbind\\\".*\""
