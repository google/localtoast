      "                        expected_regex: \"\\\\s*-w\\\\s*/sbin/insmod\\\\s*-p\\\\s*x\\\\s*-k\\\\s*modules\\\\s*\""
      "                    }"
      "                    match_criteria:{"
      "                        filter_regex: \".*/sbin/rmmod.*\""
      "                        expected_regex: \"\\\\s*-w\\\\s*/sbin/rmmod\\\\s*-p\\\\s*x\\\\s*-k\\\\s*modules\\\\s*\""
      "                    }"
      "                    match_criteria:{"
      "                        filter_regex: \".*/sbin/modprobe.*\""
      "                        expected_regex: \"\\\\s*-w\\\\s*/sbin/modprobe\\\\s*-p\\\\s*x\\\\s*-k\\\\s*modules\\\\s*\""
      "                    }"
      "                    match_criteria:{"
      "                        filter_regex: \".*-S\\\\s*delete_module\\\\s*-k\\\\s*modules\\\\s*\""
      "                        expected_regex: \"\\\\s*-a\\\\s*always,exit\\\\s*-F\\\\s*arch=b(32|64)\\\\s*-S\\\\s*init_module\\\\s*-S\\\\s*delete_module\\\\s*-k\\\\s*modules\\\\s*\""
      "                    }"
      "                }"
      "            }"
      "        }"
      "    }"
      "    image_scanning:{"
      "        check_alternatives:{"
      "            file_checks:{"
      "                files_to_check:{files_in_dir:{"
      "                    dir_path:\"/etc/audit/rules.d\""
      "                    recursive: true"
