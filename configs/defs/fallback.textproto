      "                    match_type: ALL_MATCH_ANY_ORDER"
      "                    match_criteria:{"
      "                        filter_regex: \".*-k\\\\s+access\\\\s*\""
      "                        expected_regex: \"\\\\s*-a\\\\s*always,exit\\\\s*-F\\\\s*arch=b[2346]{2}\\\\s*-S\\\\s*creat\\\\s*-S\\\\s*open\\\\s*-S\\\\s*openat\\\\s*-S\\\\s*truncate\\\\s*-S\\\\s*ftruncate\\\\s*\\\\s*-F\\\\s*exit\\\\s*=\\\\s*-(EACCES|EPERM)\\\\s*-F\\\\s*auid\\\\s*>=\\\\s*(5|10)00+\\\\s*-F\\\\s*auid\\\\!=(unset|-1|4294967295)\\\\s*-k\\\\s+access\\\\s*\""
      "                    }"
      "                    match_criteria:{"
      "                        filter_regex: \".*-k\\\\s+access\\\\s*\""
      "                        expected_regex: \"\\\\s*-a\\\\s*always,exit\\\\s*-F\\\\s*arch=b[2346]{2}\\\\s*-S\\\\s*creat\\\\s*-S\\\\s*open\\\\s*-S\\\\s*openat\\\\s*-S\\\\s*truncate\\\\s*-S\\\\s*ftruncate\\\\s*\\\\s*-F\\\\s*exit\\\\s*=\\\\s*-(EACCES|EPERM)\\\\s*-F\\\\s*auid\\\\s*>=\\\\s*(5|10)00+\\\\s*-F\\\\s*auid\\\\!=(unset|-1|4294967295)\\\\s*-k\\\\s+access\\\\s*\""
      "                    }"
      "                }"
      "            }"
      "        }"
      "    }"
      "    image_scanning:{"
      "        check_alternatives:{"
      "            file_checks:{"
      "                files_to_check:{"
      "                    files_in_dir:{"
      "                        dir_path:\"/etc/audit/rules.d\""
      "                        recursive: true"
      "                        files_only: true"
      "                        filename_regex: \".*\\\\.rules\""
      "                    }"
      "                }"
      "                content_entry:{"
      "                    match_type: ALL_MATCH_ANY_ORDER"
      "                    match_criteria:{"
      "                        filter_regex: \".*-k\\\\s+access\\\\s*\""
      "                        expected_regex: \"\\\\s*-a\\\\s*always,exit\\\\s*-F\\\\s*arch=b[2346]{2}\\\\s*-S\\\\s*creat\\\\s*-S\\\\s*open\\\\s*-S\\\\s*openat\\\\s*-S\\\\s*truncate\\\\s*-S\\\\s*ftruncate\\\\s*\\\\s*-F\\\\s*exit\\\\s*=\\\\s*-(EACCES|EPERM)\\\\s*-F\\\\s*auid\\\\s*>=\\\\s*(5|10)00+\\\\s*-F\\\\s*auid\\\\!=(unset|-1|4294967295)\\\\s*-k\\\\s+access\\\\s*\""
