      "        }"
      "      }"
      "    }"
      "    file_checks:{"
      "      files_to_check:{files_in_dir:{"
      "        dir_path:\"/etc/sysctl.d\""
      "        recursive: true"
      "      }}"
      "      content_entry:{"
      "        match_type: ALL_MATCH_ANY_ORDER"
      "        match_criteria: {"
      "          filter_regex: \"fs.suid_dumpable = .*\""
      "          expected_regex: \"fs.suid_dumpable = 0\""
      "        }"
      "      }"
      "    }"
      "    file_checks:{"
      "      files_to_check:{single_file:{path:\"/proc/sys/fs/suid_dumpable\"}}"
      "      content:{content:\"0\\n\"}"
      "    }"
      "  }"
      "  check_alternatives:{"
