      "      content_entry:{"
      "        match_type: ALL_MATCH_STRICT_ORDER"
      "        match_criteria:{"
      "          filter_regex: \"-A INPUT -i lo -j ACCEPT\""
      "          expected_regex: \"-A INPUT -i lo -j ACCEPT\""
      "        }"
      "        match_criteria:{"
      "          filter_regex: \"-A INPUT -s ::1(/128)? -j DROP\""
      "          expected_regex: \"-A INPUT -s ::1(/128)? -j DROP\""
      "        }"
      "        match_criteria:{"
