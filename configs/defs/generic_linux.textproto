      "      \"       if [ $(echo $fileperm | cut -c9) != \\\"-\\\" ]; then\\n\""
      "      \"         echo \\\"Other Write permission set on file $file\\\"\\n\""
      "      \"       fi\\n\""
      "      \"      fi\\n\""
