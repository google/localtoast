      "        }"
      "        match_criteria: {"
      "          filter_regex: \"MACs .*\""
      "          expected_regex: \"MACs .*hmac-ripemd160-etm@openssh.com(,|$).*\""
      "        }"
      "        match_criteria: {"
      "          filter_regex: \"MACs .*\""
      "          expected_regex: \"MACs .*hmac-sha1-etm@openssh.com(,|$).*\""
      "        }"
      "        match_criteria: {"
      "          filter_regex: \"MACs .*\""
      "          expected_regex: \"MACs .*hmac-sha1-96-etm@openssh.com(,|$).*\""
      "        }"
      "        match_criteria: {"
