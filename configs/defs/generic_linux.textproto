      profile_level: 1
      severity: HIGH
    }
    scan_instructions:
      "generic:{check_alternatives:{"
      "    file_checks:{"
      "      files_to_check:{files_in_dir:{dir_path:\"/etc/security/\", recursive:true}}"
      "      content_entry:{"
      "        match_type: NONE_MATCH"
      "        match_criteria: {"
