      "        }"
      "    }"
      "    image_scanning:{"
      "        check_alternatives:{"
      "            file_checks:{"
      "                files_to_check:{files_in_dir:{"
      "                    dir_path:\"/etc/audit/rules.d\""
      "                    recursive: true"
      "                    files_only: true"
      "                    filename_regex: \".*\\\\.rules\""
      "                }}"
      "                content_entry:{"
      "                    match_type: ALL_MATCH_ANY_ORDER"
      "                    match_criteria:{"
      "                        filter_regex: \".*/sbin/insmod.*\""
      "                        expected_regex: \"\\\\s*-w\\\\s*/sbin/insmod\\\\s*-p\\\\s*x\\\\s*-k\\\\s*modules\\\\s*\""
      "                    }"
      "                    match_criteria:{"
      "                        filter_regex: \".*/sbin/rmmod.*\""
