      "      files_to_check:{single_file:{path:\"/etc/nftables.rules\"}}"
      "      content_entry:{"
      "        match_type: ALL_MATCH_ANY_ORDER"
      "        match_criteria:{"
      "          filter_regex: \"\\\\s*ip6 saddr ::1 counter.*drop\""
