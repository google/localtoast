      "                    }"
      "                }"
      "            }"
      "        }"
      "        check_alternatives:{"
      "            file_checks:{"
      "                files_to_check:{single_file:{path:\"/etc/login.defs\"}}"
      "                content_entry:{"
      "                    match_type: ALL_MATCH_ANY_ORDER"
      "                    match_criteria:{"
      "                        filter_regex: \"UID_MIN.*\""
