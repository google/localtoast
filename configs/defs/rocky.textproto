      "                    }"
      "                }"
      "            }"
      "            file_checks:{"
      "                files_to_check:{single_file:{path: \"/etc/audit/audit.rules\"}}"
      "                content_entry:{"
      "                    match_type: ALL_MATCH_ANY_ORDER"
      "                    match_criteria:{"
      "                        filter_regex: \".*/etc/sudoers\\\\.d.*\""
      "                        expected_regex: \"\\\\s*-w\\\\s+/etc/sudoers\\\\.d\\\\s+-p\\\\s*wa\\\\s*(-F\\\\s*key=|-k)\\\\s*[!-~]*\\\\s*$\""
      "                    }"
      "                }"
      "            }"
