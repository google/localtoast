      "    files_to_check:{single_file:{path:\"/etc/shadow\"}}"
      "    permission:{"
      "      clear_bits: 0137"
      "      user: {name: \"root\" should_own: true}"
      "      group: {name: \"root\" should_own: true}"
      "    }"
      "  }"
      "}"
      "check_alternatives:{"
      "  file_checks:{"
      "    files_to_check:{single_file:{path:\"/etc/shadow\"}}"
      "    permission:{"
      "      clear_bits: 0137"
      "      user: {name: \"root\" should_own: true}"
